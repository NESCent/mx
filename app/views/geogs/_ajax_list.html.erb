<%= render(:partial => 'shared/page_nav_scope', :locals => {:pages => @geog_pages}) -%>
<%= render(:partial => 'shared/page_nav', :locals => {:pages => @geog_pages}) -%>
<table class="list_table">
  <%= t_col_heads( ["Name", "Country", "State", "County", "Abbrev", "FIPS", "Type", "center_lat", "center_long", "mod on", "mod by"] ) %>
  <% @geogs.each_with_index do |geog, i| -%>
  <%= t_row({ 'obj' => geog, 'tr_css' => i,
    'cell_data' =>[
    link_to(geog.name, :action => :show, :id => geog),
    geog.country == nil ? nil : geog.country.name,
    geog.state ? geog.state.name : '',
    geog.county ? geog.county.name : '',
    geog.abbreviation,
    geog.fips_code,
    (geog.geog_type.blank? ? '<i class="failed"> not given </i>' : geog.geog_type.name),
    geog.center_lat,
    geog.center_long,
    time_ago_in_words(geog.updated_on),
    geog.m_by]}) %>
    <% end -%>
  </table>
<br />
<%= render(:partial => 'shared/page_nav', :locals => {:pages => @geog_pages}) -%>
