<div class='navigator'>
  <%= navigator2(:obj => @specimen, :do => session['specimen_view'], :ord => "id") -%>

  <% if @specimen.sequences.length > 0 -%>
  <div class="item"> <%= session['specimen_view'] == 'show_seqs' ? '<span class="navigator_current">sequences</span>' :  link_to('sequences', :action => 'show_seqs', :id => @specimen)  %> </div>  
  <% end -%>

  <% if @specimen.has_lat_long %>
    <div class="item"> <%= link_to('BerkeleyMapper map', :action => 'map', :controller => 'berkeley_mapper_result', :specimen_id => @specimen.id) -%> </div>
  <% end %>

  <div class="category">functions</div>
  <div class="item"> <%= link_to('create extract', :action => :new, :controller => :extract, :specimen_id => @specimen.id) -%> </div> 
  <div class="item"> <%= link_to('clone', url_for(:action => :clone, :controller => :specimen, :id => @specimen), :confirm => "Are you sure you want to clone this specimen? Identifiers will not be cloned.") -%> </div> 


</div>
