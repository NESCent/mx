<table class="record_list">
  <%= t_col_heads( ["label", "class", {'name' => "reference", "style" => "width: 150px"}, "notes", "id", "mod by", "mod on"] ) -%>
  <% @sensus.each_with_index do |s, i| -%>
    <%=  t_row({ 'obj' => s, 'tr_css' => i, 
    'cell_data' => [ 
    link_to(s.label.display_name, :action => :show, :controller => :labels, :id => s.label),
    link_to(s.ontology_class.definition, :action => :show, :controller => :ontology_classes, :id => s.ontology_class),
    short_ref_link_tag(s.ref),
    s.notes,
    s.id,
    s.updator.display_name,
    time_ago_in_words(s.updated_on)
    ] }) -%>
  <% end -%>
</table>
