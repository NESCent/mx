<h3>Listing association supports</h3>
<%= paginate(@associations_supports, :total_count => @associations_supports.total_count) -%>
<table>
  <tr>
<% for column in AssociationSupport.content_columns %>
    <th><%= column.human_name %></th>
<% end %>
  </tr>
  
<% for association_support in @association_supports %>
  <tr>
  <% for column in AssociationSupport.content_columns %>
    <td><%=h association_support.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => association_support %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => association_support %></td>
    <td><%= link_to 'Destroy', {:action => 'destroy', :id => association_support}, :confirm => "Are you sure?" %></td>
  </tr>
<% end %>
</table>
<%= paginate(@associations_supports, :total_count => @associations_supports.total_count) -%>
