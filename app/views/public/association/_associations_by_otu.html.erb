<ul>  <% for as in @otu.association_parts %>    <% @assoc = Association.find(as.association_id) %>    <li><%= link_to render :partial => "association/part", :collection => @assoc.association_parts, :action => 'show', :id => @assoc.id, :controller => 'association' %></li>    <% @supports = AssociationSupport.find(:all, :conditions => ["association_id = ?", @assoc.id], :include => :confidence, :order => "confidences.position") %>    <% @row_type ||= '' %>    <%= render(:partial => "association/support_table#{@row_type}") -%>  <% end %></ul>