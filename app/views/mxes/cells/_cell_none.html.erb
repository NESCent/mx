<%# all _cell partials require the locals: codings = [], mx_id, i, j, o = Otu, c = Chr -%>
<% if codings.size == 0 -%>
  <div style="background-color:#ddd; color: red; width: 9px; font-weight: bolder; margin: 0;">
  <%= link_to("-".html_safe,  {:action => :_cell_zoom, :mx_id => mx_id, :chr_id => c.id, :otu_id => o.id, :x => i, :y => j}, :remote => true, :method => 'get'  )  -%>
  </div>
<% elsif codings.size == 1 -%>
  <%= link_to(codings.first.display_name(:type => :windowed_value).html_safe, {:action => :_cell_zoom, :mx_id => mx_id, :chr_id => c.id, :otu_id => o.id, :x => i, :y => j},  :remote => true, :method => 'get' )  -%>
<% else -%>
   <div style="background-color:#e5993a; width: 12px; font-weight: bolder; margin: 0;">
  <%= link_to("#".html_safe, {:action => :_cell_zoom, :mx_id => mx_id, :chr_id => c.id, :otu_id => o.id, :x => i, :y => j}, :remote => true, :method => 'get'  )  -%>
  </div>
<% end -%>
